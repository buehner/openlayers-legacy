{
  "name": "@geoext/openlayers-legacy",
  "version": "6.5.1",
  "description": "OpenLayers legacy build for geoext",
  "scripts": {
    "build": "rm -rf dist && cd node_modules/ol/ && npm i && npm run build-legacy && cp -r build/legacy ../../dist && cp LICENSE.md ../../dist/ && cd ../../",
    "release": "npm run build && np 6.5.2 --no-yarn"
  },
  "keywords": [
    "geoext",
    "openlayers"
  ],
  "author": "Simon Seyock <seyock@terrestris.de>",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "np": "^7.4.0",
    "ol": "git+https://github.com/openlayers/openlayers.git#v6.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com:geoext/openlayers-legacy.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ]
}
