{
  "name": "@geoext/openlayers-legacy",
  "version": "6.5.0",
  "description": "openlayers legacy build for geoext",
  "scripts": {
    "build": "rm -rf build && cd node_modules/ol/ && npm i && npm run build-legacy && cp -r build/legacy ../../build && cp LICENSE.md ../../build/ && cd ../../ && cp package.json build/",
    "release": "np 6.5.0 --contents=build --no-yarn"
  },
  "keywords": [
    "geoext",
    "openlayers"
  ],
  "author": "Simon Seyock <seyock@terrestris.de>",
  "license": "BSD-2-Clause",
  "dependencies": {
    "np": "^7.4.0",
    "ol": "git+https://github.com/openlayers/openlayers.git#v6.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com:geoext/openlayers-legacy.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
